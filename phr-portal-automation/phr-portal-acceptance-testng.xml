		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="Phr-Portal-Acceptance-Automation">
		<groups>
			<run>
				<include name="AcceptanceTests" />
			</run>
		</groups>

		<classes>
			<class name="com.intuit.ihg.product.phr.test.PhrAcceptanceTests" />

		</classes>


	</test>
</suite>