<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
 	
 	<parent>
		<groupId>com.intuit.ihg</groupId>
		<artifactId>ihg-depot-qa-parent-pom</artifactId>
		<version>19.4.0</version>
	</parent>
	
	<artifactId>object-maps</artifactId>
	<packaging>jar</packaging>
	<name>object-maps</name>
  
  	<repositories>
        <repository>
            <id>IHG-nexus</id>
            <name>IHG-nexus</name>
            <url>http://maven.qhg.local/nexus/content/groups/qhg-dev/</url>
        </repository>
    </repositories>
    
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<ihg.version>19.4.0</ihg.version>
	</properties>
  
	<dependencies>
		<dependency>
  			<groupId>com.intuit.ihg</groupId>
  			<artifactId>qa-automation-utils</artifactId>
  			<version>${ihg.version}</version>
  		</dependency>
  		<dependency>
  			<groupId>com.intuit.ihg</groupId>
  			<artifactId>practiceportal-api</artifactId>
  			<version>${ihg.version}</version>
  		</dependency>
  		<dependency>
  			<groupId>com.intuit.ihg</groupId>
  			<artifactId>patientportal1-api</artifactId>
  			<version>${ihg.version}</version>
  		</dependency>
  		<dependency>
  			<groupId>com.intuit.ihg</groupId>
  			<artifactId>patientportal1-object-maps</artifactId>
  			<version>${ihg.version}</version>
  		</dependency>
  	</dependencies>
  	
    <build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.3.2</version>
				<executions>
					<execution>
						<phase>package</phase>
						<goals>
							<goal>test-jar</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<outputDirectory>target/autolab</outputDirectory>
					<archive>
						<manifest>
							<mainClass>com.intuit.ifs.csscat.core.RunTestNGSuite</mainClass>
						</manifest>
					</archive>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>