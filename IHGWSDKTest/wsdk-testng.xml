<?xml version="1.0" encoding="UTF-8"?>

<suite name="Medfusion Integration Test suite" verbose="1">
    <listeners>
        <listener class-name="com.intuit.ifs.csscat.core.TestValidationErrorListener" />
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>

    <test name="Medfusion Integration Test">
        <classes>
            <class name="net.medfusion.integrations.webservices.MedfusionWebservicesTest"/>
            <class name="net.medfusion.integrations.webservices.IntegrationEngineTest"/>
        </classes>
    </test>
</suite>
