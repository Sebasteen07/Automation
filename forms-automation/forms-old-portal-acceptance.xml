<?xml version="1.0" encoding="UTF-8"?>
<suite name="forms-old-portal-acceptance" verbose="2">
    <listeners>
        <listener class-name="com.intuit.ifs.csscat.core.TestValidationErrorListener"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>

    <test name="forms-old-portal">
        <groups>
            <run>
                <include name="OldPortalForms"/>
            </run>
        </groups>
        <classes>
            <class name="com.intuit.ihg.product.forms.test.Portal1FormsAcceptanceTests"/>
            <class name="com.intuit.ihg.product.forms.test.CalculatedFormsAcceptanceTest"/>
        </classes>
    </test>
</suite>
