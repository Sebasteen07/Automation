<?xml version="1.0" encoding="UTF-8"?>
<suite name = "MOBILE-SMOKE-AUTOMATION-SUITE" verbose = "2" >
<listeners>
  <listener class-name="com.intuit.ifs.csscat.core.TestValidationErrorListener" />
  <listener class-name="org.uncommons.reportng.HTMLReporter" />
  <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>

<test name = "Mobile-Smoke-Automation" >
 <groups>
    <run>
      <include name="AcceptanceTests"  />
      <exclude name="SmokeTests"  />
    </run>
</groups>
<classes>
    <class name="com.intuit.ihg.product.mobile.test.MobileAcceptanceTestPlan"/>
  </classes>
</test>
</suite>